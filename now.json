{
  "version": 2,
  "public": false,
  "name": "math",
  "alias": "math.now.sh",
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node-server",
      "config": {
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "thirdparty/MathJax/unpacked/**",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/mathjax/(.*)",
      "dest": "/thirdparty/MathJax/unpacked/$1",
      "headers": { "Content-Type": "text/plain" }
    },
    { "src": "/(.*)", "dest": "index.js" }
  ],
  "github": {
    "enabled": false,
    "autoAlias": false
  }
}
