{
  "version": 2,
  "name": "math",
  "alias": "math.now.sh",
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node-server",
      "config": {
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "index.html",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js",
      "headers": { "cache-control": "s-maxage=3600" }
    }
  ],
  "github": {
    "enabled": true,
    "autoAlias": true
  }
}
